!Terminal color
URxvt.font: xft:Cousine-Regular:size=15
URxvt.letterSpace: -1

!Terminal extensions
URxvt.perl-ext-common: tabbed,resize-font,clipboard

!Terminal tab settings
!
!How to use:
!Key 		Description
!
!Shift+Down 	New tab
!Shift+Left 	Go to left tab
!Shift+Right 	Go to right tab
!Ctrl+Left 	Move tab to the left
!Ctrl+Right 	Move tab to the right
!Ctrl+d 	Close tab 
URxvt.tabbed.tabbar-fg: 2
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 3
URxvt.tabbed.tab-bg: 0

!Termilal cut and paste

!URxvt.clipboard.autocopy: true
!URxvt.keysym.M-c: perl:clipboard:copy
!URxvt.keysym.M-v: perl:clipboard:paste

